mixin newsWrrap(v, n, spec, t, text)
	.col-md-6.col-lg-4.col-xl-3
		+b.newsItem&attributes(attributes)
			+pic(`newsPre-${n}.jpg`, 'bg', 0)
			+e.bookmark(href="#")
				+icon(`bookmarkActive`)
			+e.content 
				+e.specialization!= spec
				+e.SPAN.title
					+e.title(href="#")!= t
				+e.P.text!= text
			if v
				+videoCollection()

