mixin videoCollection()
	+b.video-collection(href="#")
		+icon('playVideo')
		span Видеолекция

mixin newsWrrap(n, spec, t, text)
	+b.newsItem&attributes(attributes)
		+pic(`newsPre-${n}.jpg`, 'bg', 0)
		+e.bookmark(href="#")
			+icon(`bookmarkActive`)
		+e.content 
			+e.specialization!= spec
			+e.SPAN.title
				+e.title(href="#")!= t
			+e.P.text!= text
		+videoCollection()

