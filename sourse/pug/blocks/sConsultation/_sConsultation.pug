mixin consultation(i, n, d, t, text)
	+e.LI.item
		+pic(`sTeam-${i}.jpg`)
		+e.head 
			+e.SPAN.name!= n 
			+e.SPAN.date!= d
		+e.body
			+e.SPAN.title!= t 
			p!= text
			+e.btn(href="#") 
				+icon('shevron-down')
				span Показать ответ
		+consultAnswer(n, "Даниил Маркарьян", d, "Здравствуйте <br> <br> Выделения различного характера возможны в период адаптации организма к препарату ( 1-3 месяца идет период адаптации). ")

mixin consultAnswer(n, nDoctor, d, content)
	+e.answer
		+e.reply
			+icon('northWestArrow')
			span!= n
		+e.answerBody
			+pic(`sTeam-2.jpg`)
			+e.who
				+e.SPAN.name--color!= nDoctor
				+e.SPAN.date!= d
				+e.SPAN.position Психолог
			+e.body
				p!= content


